{
  "name": "edgedb",
  "description": "A JavaScript binding for EdgeDB",
  "author": "EdgeDB <info@edgedb.com>",
  "engines": {
    "node": ">= 10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edgedb/edgedb-js.git"
  },
  "version": "0.5.0-alpha",
  "main": "./dist/src/index.js",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "/dist",
    "/src"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^13.1.6",
    "get-stdin": "^7.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "tslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "gen-errors": "edb gen-errors-json --client | node tools/gen_errors.js"
  }
}
